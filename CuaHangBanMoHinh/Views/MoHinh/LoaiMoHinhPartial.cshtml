
@model IEnumerable<CuaHangBanMoHinh.Models.EL.LOAIMOHINH>
<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">DANH MỤC SẢN PHẨM</li>
    @{
        foreach (var item in Model)
        {
        <li class="list-group-item">
            <ion-icon name="folder-outline"></ion-icon>
            @Html.ActionLink(item.TenMH, "SanPhamTheoDanhMuc", new { idLoaiMH = item.MaLoaiMH }, "MoHinh")
        </li>
        }
    }
</ul>
